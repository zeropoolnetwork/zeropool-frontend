{
  "name": "zeropool",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://zeropoolnetwork.github.io/zeropool-frontend/",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen --watch",
    "test:coverage": "react-scripts test --env=jest-environment-jsdom-sixteen --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build --remote zp",
    "storybook": "start-storybook -p 6006 -s public --no-dll",
    "build-storybook": "build-storybook -s public --no-dll"
  },
  "dependencies": {
    "@bem-react/classname": "^1.5.8",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "connected-react-router": "^6.8.0",
    "fontsource-exo-2": "^3.0.5",
    "gh-pages": "^3.1.0",
    "history": "^4.10.1",
    "node-sass": "^4.14.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hook-form": "^6.11.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.4",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "redux-persist": "^6.0.0",
    "reselect": "^4.0.0",
    "typesafe-actions": "^5.1.0",
    "typescript": "~3.7.2",
    "wallet.ts": "1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@hookform/devtools": "^2.2.1",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/node-logger": "^6.0.28",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.28",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux": "^3.6.0",
    "babel-loader": "^8.1.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "mutationobserver-shim": "^0.3.7",
    "react-is": "^17.0.1",
    "ts-jest": "^26.4.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/about/**/*.{js,jsx,ts,tsx}",
      "src/common/**/*.{js,jsx,ts,tsx}",
      "src/register/**/*.{js,jsx,ts,tsx}",
      "src/wallet/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
