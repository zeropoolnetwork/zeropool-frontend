{
  "name": "zeropool",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://zeropoolnetwork.github.io/zeropool-frontend/",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen --watch",
    "test:coverage": "react-scripts test --env=jest-environment-jsdom-sixteen --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build --remote zp",
    "storybook": "start-storybook -p 6006 -s public --no-dll",
    "build-storybook": "build-storybook -s public --no-dll",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint": "tslint -c tslint.json src/**/*.{ts,tsx} --fix --format verbose"
  },
  "dependencies": {
    "@bem-react/classname": "^1.5.8",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "axios": "^0.21.0",
    "axios-observable": "^1.1.3",
    "connected-react-router": "^6.8.0",
    "fontsource-exo-2": "^3.0.5",
    "gh-pages": "^3.1.0",
    "history": "^4.10.1",
    "jssha": "^3.2.0",
    "node-sass": "^4.14.1",
    "notistack": "^1.0.3",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hook-form": "^6.11.0",
    "react-number-format": "^4.4.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.4",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "redux-persist": "^6.0.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.6.3",
    "typesafe-actions": "^5.1.0",
    "typescript": "^4.1.0",
    "wallet.ts": "1.0.1",
    "zeropool-api-js": "github:zeropoolnetwork/zeropool-api-js"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@hookform/devtools": "^2.2.1",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/node-logger": "^6.0.28",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.28",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/qrcode.react": "^1.0.1",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux": "^3.6.0",
    "babel-loader": "8.1.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "mutationobserver-shim": "^0.3.7",
    "prettier": "2.2.1",
    "react-is": "^17.0.1",
    "ts-jest": "^26.4.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/shared/**/*.{ts,tsx}",
      "src/register/**/*.{ts,tsx}",
      "src/wallet/**/*.{ts,tsx}",
      "!src/**/*.stories.tsx"
    ]
  }
}
